.price {
  color: #E92E3A;
  font-weight: 700;
}
.line-2-ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.line-1-ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.mix-1px-bottom {
  position: relative;
}
.mix-1px-bottom:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 300%;
  height: 300%;
  border: 0 #ccc solid;
  box-sizing: border-box;
  transform-origin: 0 0;
  transform: scale(0.33333333);
  pointer-events: none;
  z-index: 0;
  border-bottom-width: 1px;
}
.mix-1px-bottom-top {
  position: relative;
}
.mix-1px-bottom-top:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 300%;
  height: 300%;
  border: 0 #ccc solid;
  box-sizing: border-box;
  transform-origin: 0 0;
  transform: scale(0.33333333);
  pointer-events: none;
  z-index: 0;
  border-top-width: 1px;
  border-bottom-width: 1px;
}
.mix-1px-bottom-1 {
  border-bottom: solid 1px #F0F0F0;
}
.mix-1px-top-1 {
  border-top: solid 1px #F0F0F0;
}
.mix-1px-box {
  border: solid 1px #F0F0F0;
}
.bg-white {
  background-color: #fff;
}
.bold-5 {
  font-weight: 500;
}
.margin-top-10 {
  margin-top: 10rpx;
}
.margin-top-20 {
  margin-top: 20rpx;
}
.button-style-1 {
  background-color: #3A84EF;
  border-radius: 20rpx;
  border-color: #3A84EF;
  box-shadow: 3rpx 3rpx 8rpx #3A84EF;
  color: #fff;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.button-style-1::after {
  border: none;
}
.appbar {
  margin-top: 1px;
  border-bottom: solid 1px #F0F0F0;
}
navigator {
  width: 100%;
}
.column {
  display: flex;
  flex-direction: column;
}
.row {
  display: flex;
  flex-direction: row;
}
.store {
  background-color: #FAFAFA;
}
.store .head {
  position: relative;
  z-index: 1;
  background-color: #3A84EF;
  box-shadow: 0 5rpx 5rpx 0 rgba(87, 129, 193, 0.16), 0 5rpx 10rpx 0 rgba(76, 129, 198, 0.13);
}
.store .head .row1 {
  margin-right: 20rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.store .head .row1 .image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.store .head .row1 .info {
  flex-grow: 1;
  align-content: space-around;
}
.store .head .row1 .info .store-bar {
  justify-content: space-between;
  align-items: center;
}
.store .head .row1 .info .store-bar .name {
  color: #fbf8fd;
  font-size: 12pt;
  margin-right: 10rpx;
}
.store .head .row1 .info .store-bar .living {
  color: #fff;
  font-size: 9pt;
}
.store .head .row1 .info .attention {
  margin-top: 10rpx;
  justify-content: space-between;
}
.store .head .row1 .info .attention .left {
  align-items: center;
}
.store .head .row1 .info .attention .left .tag {
  margin-right: 20rpx;
  font-size: 8pt;
  font-weight: 400;
  padding: 2rpx 20rpx;
  border-radius: 50rpx;
  background-color: #0063ee;
  color: #fff;
}
.store .head .row1 .info .attention .left .number {
  color: #fdfcff;
  font-size: 9pt;
}
.store .head .row1 .info .attention .btn {
  background-color: #0063ee;
  color: #fff;
  padding: 0 20rpx;
  border-radius: 50rpx;
}
.store .footer {
  background-color: #fff;
  width: 100%;
  position: fixed;
  height: 100rpx;
  bottom: 0;
  justify-content: space-between;
  text-align: center;
  align-items: center;
  border-top: solid 1px #f0f0f0;
}
.store .footer .store-link,
.store .footer .classify {
  flex-grow: 1;
  border-right: solid 1px #f0f0f0;
}
.store .footer .link {
  width: 100rpx;
}
.sticky-in-relative-container {
  border-bottom: solid 1px #F0F0F0;
}
.sticky-in-relative-container .nav_top {
  background: #fff;
  font-size: 14px;
  color: #878787;
}
.sticky-in-relative-container .nav_top .ui-col.active {
  color: #09C2B1;
}
.sticky-in-relative-container .nav_top .ui-row {
  width: 100%;
  height: 100%;
}
.sticky-in-relative-container .nav_top .ui-row view {
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
