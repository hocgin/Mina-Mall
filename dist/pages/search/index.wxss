.price {
  color: #E92E3A;
  font-weight: 700;
}
.bar-title {
  font-size: 12pt;
  font-weight: 450;
}
.y-center {
  align-items: center;
}
.line-2-ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.line-1-ellipsis {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
}
.mix-1px-bottom {
  position: relative;
}
.mix-1px-bottom:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 300%;
  height: 300%;
  border: 0 #ccc solid;
  box-sizing: border-box;
  transform-origin: 0 0;
  transform: scale(0.33333333);
  pointer-events: none;
  z-index: 0;
  border-bottom-width: 1px;
}
.mix-1px-bottom-top {
  position: relative;
}
.mix-1px-bottom-top:before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 300%;
  height: 300%;
  border: 0 #ccc solid;
  box-sizing: border-box;
  transform-origin: 0 0;
  transform: scale(0.33333333);
  pointer-events: none;
  z-index: 0;
  border-top-width: 1px;
  border-bottom-width: 1px;
}
.mix-1px-bottom-1 {
  border-bottom: solid 1px #F0F0F0;
}
.mix-1px-top-1 {
  border-top: solid 1px #F0F0F0;
}
.mix-1px-box {
  border: solid 1px #F0F0F0;
}
