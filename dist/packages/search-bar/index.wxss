.search-bar {
  width: 100%;
  box-sizing: border-box;
  background-color: rgba(255, 255, 255, 0.3);
  display: flex;
  align-items: center;
  color: #B5B5BB;
  padding-bottom: 15rpx;
  padding-top: 15rpx;
  border-radius: 40rpx;
}
.search-bar .icon {
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.search-bar .tips {
  font-size: 11pt;
}
